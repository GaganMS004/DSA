# Remove Duplicates in-place from Sorted Array

def remove_duplicates(arr):
    if len(arr) == 0:
        return 0
    p = 0
    for q in range(1, len(arr)):
        if arr[p] != arr[q]:
            p += 1
            arr[p] = arr[q] 
    return p + 1


arr = [1, 2, 2, 4, 5, 6, 6, 6, 7, 7]
k = remove_duplicates(arr)

for i in range(k):
    print(arr[i], end=" ")
