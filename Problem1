# Problem Statement: Given an array of N integers, left rotate the array by one place.

def left_rotate(arr):
    if len(arr) <= 1:
        return arr
    first = arr[0]
    for i in range(len(arr) - 1):
        arr[i] = arr[i + 1] 
    arr[-1] = first
    return arr

arr = [2, 4, 6, 5, 1]
left_rotate(arr)

for num in arr:
    print(num, end=" ")
